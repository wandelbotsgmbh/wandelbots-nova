{
  "$defs": {
    "Pose": {
      "description": "A pose (position and orientation)\n\nExample:\n>>> Pose(position=Vector3d(x=10, y=20, z=30), orientation=Vector3d(x=1, y=2, z=3))\nPose(position=Vector3d(x=10, y=20, z=30), orientation=Vector3d(x=1, y=2, z=3))",
      "properties": {
        "position": {
          "$ref": "#/$defs/Vector3d"
        },
        "orientation": {
          "$ref": "#/$defs/Vector3d"
        }
      },
      "required": [
        "position",
        "orientation"
      ],
      "title": "Pose",
      "type": "object"
    },
    "Vector3d": {
      "description": "A vector 3d class\nExamples:\n>>> Vector3d(x=10, y=20, z=30)\nVector3d(x=10, y=20, z=30)",
      "properties": {
        "x": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "type": "integer"
            }
          ],
          "title": "X"
        },
        "y": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "type": "integer"
            }
          ],
          "title": "Y"
        },
        "z": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "type": "integer"
            }
          ],
          "title": "Z"
        }
      },
      "required": [
        "x",
        "y",
        "z"
      ],
      "title": "Vector3d",
      "type": "object"
    }
  },
  "properties": {
    "number_of_picks": {
      "description": "Number of picks to perform",
      "exclusiveMinimum": 0,
      "title": "Number Of Picks",
      "type": "integer"
    },
    "speed_factor": {
      "default": 1.0,
      "description": "Speed multiplier for movements",
      "maximum": 2.0,
      "minimum": 0.1,
      "title": "Speed Factor",
      "type": "number"
    },
    "robot_name": {
      "description": "Name of the robot to control",
      "pattern": "^[A-Za-z0-9_-]+$",
      "title": "Robot Name",
      "type": "string"
    },
    "enable_logging": {
      "default": false,
      "description": "Enable detailed motion logging",
      "title": "Enable Logging",
      "type": "boolean"
    },
    "waypoint_indices": {
      "description": "Indices of waypoints to visit",
      "items": {
        "type": "integer"
      },
      "maxItems": 10,
      "minItems": 1,
      "title": "Waypoint Indices",
      "type": "array"
    },
    "operation_mode": {
      "default": "standard",
      "description": "Operation mode for the program",
      "pattern": "^(standard|advanced|debug)$",
      "title": "Operation Mode",
      "type": "string"
    },
    "home_pose": {
      "$ref": "#/$defs/Pose",
      "description": "Home position coordinates (x, y, z)"
    }
  },
  "required": [
    "number_of_picks",
    "robot_name",
    "home_pose"
  ],
  "title": "ProgramParameter",
  "type": "object"
}